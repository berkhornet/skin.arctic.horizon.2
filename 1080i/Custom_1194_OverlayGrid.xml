<?xml version="1.0" encoding="UTF-8"?>
<window type="dialog" id="1194">
    <zorder>10</zorder>
    <visible>Skin.HasSetting(DebugGrid)</visible>
    <controls>

        <include content="Object_Control" condition="Skin.HasSetting(DebugGrid)">
            <param name="control">group</param>
            <!-- Edges -->
            <include content="Object_Control" condition="false">
                <param name="control">group</param>
                <control type="image">
                    <texture colordiffuse="33ff0000">common/white.png</texture>
                    <width>80</width>
                </control>
                <control type="image">
                    <texture colordiffuse="33ff0000">common/white.png</texture>
                    <width>80</width>
                    <right>0</right>
                </control>
                <control type="image">
                    <texture colordiffuse="33ff0000">common/white.png</texture>
                    <height>80</height>
                    <left>80</left>
                    <right>80</right>
                </control>
                <control type="image">
                    <texture colordiffuse="33ff0000">common/white.png</texture>
                    <height>80</height>
                    <left>80</left>
                    <right>80</right>
                    <bottom>0</bottom>
                </control>
                <control type="image">
                    <texture colordiffuse="33ff0000">common/white.png</texture>
                    <height>80</height>
                    <left>80</left>
                    <right>80</right>
                    <top>120</top>
                </control>
                <control type="image">
                    <texture colordiffuse="33ff0000">common/white.png</texture>
                    <height>80</height>
                    <left>80</left>
                    <right>80</right>
                    <bottom>120</bottom>
                </control>
                <control type="image">
                    <texture colordiffuse="33ff0000">common/white.png</texture>
                    <height>80</height>
                    <left>80</left>
                    <right>80</right>
                    <bottom>520</bottom>
                </control>
            </include>

            <include content="Object_Control" condition="false">
                <param name="control">group</param>
                <control type="image">
                    <texture colordiffuse="3300ffff">common/white.png</texture>
                    <top>200</top>
                    <left>80</left>
                    <right>80</right>
                    <height>320</height>
                </control>
                <control type="image">
                    <texture colordiffuse="3300ffff">common/white.png</texture>
                    <bottom>200</bottom>
                    <left>80</left>
                    <right>80</right>
                    <height>320</height>
                </control>
                <control type="image">
                    <texture colordiffuse="3300ffff">common/white.png</texture>
                    <bottom>80</bottom>
                    <left>80</left>
                    <right>80</right>
                    <height>40</height>
                </control>
                <control type="image">
                    <texture colordiffuse="3300ffff">common/white.png</texture>
                    <top>80</top>
                    <left>80</left>
                    <right>80</right>
                    <height>40</height>
                </control>
            </include>

            <!-- Vertical 40 px lines -->
            <control type="grouplist">
                <itemgap>39</itemgap>
                <orientation>vertical</orientation>
                <include>Vert_Red_Line_5</include>
                <include>Vert_Red_Line_5</include>
                <include>Vert_Red_Line_5</include>
                <include>Vert_Red_Line_5</include>
                <include>Vert_Red_Line_5</include>
                <include>Vert_Red_Line</include>
                <include>Vert_Red_Line</include>
            </control>

            <!-- Horizontal 40 px lines -->
            <control type="grouplist">
                <itemgap>39</itemgap>
                <orientation>horizontal</orientation>
                <include>Horz_Red_Line_5</include>
                <include>Horz_Red_Line_5</include>
                <include>Horz_Red_Line_5</include>
                <include>Horz_Red_Line_5</include>
                <include>Horz_Red_Line_5</include>
                <include>Horz_Red_Line_5</include>
                <include>Horz_Red_Line_5</include>
                <include>Horz_Red_Line_5</include>
                <include>Horz_Red_Line_5</include>
                <include>Horz_Red_Line</include>
                <include>Horz_Red_Line</include>
                <include>Horz_Red_Line</include>
            </control>

            <!-- Screen Padding Lines -->
            <control type="image">
                <top>80</top>
                <height>1</height>
                <texture colordiffuse="red">common/white.png</texture>
            </control>
            <control type="image">
                <bottom>80</bottom>
                <height>1</height>
                <texture colordiffuse="red">common/white.png</texture>
            </control>
            <control type="image">
                <left>80</left>
                <width>1</width>
                <texture colordiffuse="red">common/white.png</texture>
            </control>
            <control type="image">
                <right>80</right>
                <width>1</width>
                <texture colordiffuse="red">common/white.png</texture>
            </control>

            <!-- Outer Border Lines -->
            <control type="image">
                <top>120</top>
                <height>1</height>
                <texture colordiffuse="red">common/white.png</texture>
            </control>
            <control type="image">
                <bottom>120</bottom>
                <height>1</height>
                <texture colordiffuse="red">common/white.png</texture>
            </control>

            <!-- Inner Border Lines -->
            <control type="image">
                <top>200</top>
                <height>1</height>
                <texture colordiffuse="red">common/white.png</texture>
            </control>
            <control type="image">
                <bottom>200</bottom>
                <height>1</height>
                <texture colordiffuse="red">common/white.png</texture>
            </control>

            <!-- Trough -->
            <control type="image">
                <top>65</top>
                <height>1</height>
                <texture colordiffuse="green">common/white.png</texture>
            </control>
            <control type="image">
                <bottom>65</bottom>
                <height>1</height>
                <texture colordiffuse="green">common/white.png</texture>
            </control>
            <control type="image">
                <top>135</top>
                <height>1</height>
                <texture colordiffuse="green">common/white.png</texture>
            </control>
            <control type="image">
                <bottom>135</bottom>
                <height>1</height>
                <texture colordiffuse="green">common/white.png</texture>
            </control>

            <control type="image">
                <top>70</top>
                <height>1</height>
                <texture colordiffuse="green">common/white.png</texture>
            </control>
            <control type="image">
                <bottom>70</bottom>
                <height>1</height>
                <texture colordiffuse="green">common/white.png</texture>
            </control>
            <control type="image">
                <top>130</top>
                <height>1</height>
                <texture colordiffuse="green">common/white.png</texture>
            </control>
            <control type="image">
                <bottom>130</bottom>
                <height>1</height>
                <texture colordiffuse="green">common/white.png</texture>
            </control>


            <!-- Center Lines -->
            <control type="image">
                <top>520</top>
                <height>1</height>
                <texture colordiffuse="red">common/white.png</texture>
            </control>
            <control type="image">
                <bottom>520</bottom>
                <height>1</height>
                <texture colordiffuse="red">common/white.png</texture>
            </control>
            <control type="image">
                <centertop>50%</centertop>
                <height>1</height>
                <texture colordiffuse="blue">common/white.png</texture>
            </control>
            <control type="image">
                <centerleft>50%</centerleft>
                <width>1</width>
                <texture colordiffuse="blue">common/white.png</texture>
            </control>

        </include>
    </controls>
</window>