		<value condition="Integer.IsEqual(VideoPlayer.ChannelNumberLabel,491)">special://userdata/customisations\PVR\PVR Logo Pack\5COPS SD.png</value>
		<value condition="Integer.IsEqual(VideoPlayer.ChannelNumberLabel,492)">special://userdata/customisations\PVR\PVR Logo Pack\5EXPLORING BRITAIN SD.png</value>
		<value condition="Integer.IsEqual(VideoPlayer.ChannelNumberLabel,493)">special://userdata/customisations\PVR\PVR Logo Pack\CLASSIC TV SD.png</value>
		<value condition="Integer.IsEqual(VideoPlayer.ChannelNumberLabel,494)">special://userdata/customisations\PVR\PVR Logo Pack\GREAT BRITISH DRAMA SD.png</value>
		<value condition="Integer.IsEqual(VideoPlayer.ChannelNumberLabel,495)">special://userdata/customisations\PVR\PVR Logo Pack\SHERLOCK SD.png</value>
		<value>$INFO[Player.Art(icon)]</value>
	</variable>
	
	<!-- PVR005 - START EPG Artwork from TMDBHelper and Skin Helper Service -->	
	<variable name="Image_EPG_PVRArtwork">
		<!-- 0. No information available -->
		<value condition="System.Platform.Android">$INFO[ListItem.Icon]</value>
		<value condition="$EXP[Standard_Channel] + !Player.HasMedia + String.IsEqual(ListItem.Label,Stay tuned for next broadcast)">$INFO[ListItem.Icon]</value>
		<value condition="$EXP[Standard_Channel] + !Player.HasMedia + String.IsEqual(ListItem.Label,Channel Off Air)">$INFO[ListItem.Icon]</value>
		<value condition="$EXP[Standard_Channel] + !Player.HasMedia + String.Contains(ListItem.Label,Indian Premier League)">special://userdata/customisations\PVR\EPG Artwork\IPL2.jpg</value>
		<value condition="$EXP[Standard_Channel] + !Player.HasMedia + String.Contains(ListItem.Label,IPL )">special://userdata/customisations\PVR\EPG Artwork\IPL2.jpg</value>
		<value condition="$EXP[Standard_Channel] + !Player.HasMedia + String.Contains(ListItem.Label,EPL)">special://userdata/customisations\PVR\EPG Artwork\EPL.jpg</value>
		<value condition="$EXP[Standard_Channel] + !Player.HasMedia + String.Contains(ListItem.Label,Premier League)">special://userdata/customisations\PVR\EPG Artwork\EPL.jpg</value>
