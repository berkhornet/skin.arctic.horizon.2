	<!-- BACK006: START Holiday Theme Expressions -->
	<expression name="EnableHolidayThemes">![VideoPlayer.IsFullscreen | Window.IsActive(fullscreeninfo) | Window.IsVisible(visualisation) | Window.IsVisible(1104)]</expression>
	<!-- Christmas Time from 1. Advent to 2. Christmas Date, fix end date (26.12.) -->
	<!--expression name="ChristmasTime">Skin.HasSetting(UseHolidayThemes)</expression -->
	<expression name="ChristmasTime">System.Date(12-24,01-01) + Skin.HasSetting(UseHolidayThemes)</expression>
	<expression name="ChristmasTheme">$EXP[ChristmasTime] + $EXP[EnableHolidayThemes]</expression>
	<!-- Valentines Day, fix date (14.02) -->
	<expression name="ValentineTime">System.Date(02-14,02-15) + Skin.HasSetting(UseHolidayThemes)</expression>
	<expression name="ValentineTheme">$EXP[ValentineTime] + $EXP[EnableHolidayThemes]</expression>
	<!-- Halloween, fix date (31.10) -->
	<expression name="HalloweenTime">System.Date(10-31,11-01) + Skin.HasSetting(UseHolidayThemes)</expression>
	<expression name="HalloweenTheme">$EXP[HalloweenTime] + $EXP[EnableHolidayThemes]</expression>
	<!-- BACK006: END Holiday Theme Expressions -->

