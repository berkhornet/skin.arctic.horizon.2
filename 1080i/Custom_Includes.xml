		<value condition="$EXP[Standard_Channel] + !Player.HasMedia + String.Contains(ListItem.Label,Scottish Premiership)">special://userdata/customisations\PVR\EPG Artwork\SPFL.jpg</value>
		<value condition="$EXP[Standard_Channel] + !Player.HasMedia + String.Contains(ListItem.Label,LaLiga)">special://userdata/customisations\PVR\EPG Artwork\LaLiga.jpg</value>
		<value condition="$EXP[Standard_Channel] + !Player.HasMedia + String.Contains(ListItem.Label,La Liga)">special://userdata/customisations\PVR\EPG Artwork\LaLiga.jpg</value>
		<value condition="$EXP[Standard_Channel] + !Player.HasMedia + String.Contains(ListItem.Label,Serie A)">special://userdata/customisations\PVR\EPG Artwork\Serie A.jpg</value>
