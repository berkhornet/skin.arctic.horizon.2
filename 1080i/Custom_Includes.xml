		<value condition="$EXP[Standard_Channel] + !Player.HasMedia + String.Contains(ListItem.Label,Ligue 1)">special://userdata/customisations\PVR\EPG Artwork\Ligue1.jpg</value>
		<value condition="$EXP[Standard_Channel] + !Player.HasMedia + String.Contains(ListItem.Label,Bundesliga)">special://userdata/customisations\PVR\EPG Artwork\Bundesliga.jpg</value>
		<value condition="$EXP[Standard_Channel] + !Player.HasMedia + String.Contains(ListItem.Label, NRL)">special://userdata/customisations\PVR\EPG Artwork\NRL.jpg</value>
		<value condition="$EXP[Standard_Channel] + !Player.HasMedia + String.Contains(ListItem.Label, Baseball)">special://userdata/customisations\PVR\EPG Artwork\MLB.jpg</value>
		<value condition="$EXP[247_Channel] + !Player.HasMedia">$INFO[ListItem.Icon]</value>
		<!-- 1. ClearArt -->
	    <value condition="$EXP[Standard_Channel] + !Player.HasMedia + !String.IsEmpty(Container(99950).ListItem.Property(base_clearart))">$INFO[Container(99950).ListItem.Property(base_clearart)]</value>
        <value condition="$EXP[Standard_Channel] + !Player.HasMedia + !String.IsEmpty(Container(99950).ListItem.Art(clearart))">$INFO[Container(99950).ListItem.Art(clearart)]</value>
		<value condition="$EXP[Standard_Channel] + !Player.HasMedia + !String.IsEmpty(Window(Home).Property(SkinHelper.ListItem.Art.ClearArt))">$INFO[Window(Home).Property(SkinHelper.ListItem.Art.ClearArt)]</value>
		<!-- 2. Landscape -->
		<value condition="$EXP[Standard_Channel] + !Player.HasMedia + !String.IsEmpty(Container(99950).ListItem.Property(base_landscape))">$INFO[Container(99950).ListItem.Property(base_landscape)]</value>
        <value condition="$EXP[Standard_Channel] + !Player.HasMedia + !String.IsEmpty(Container(99950).ListItem.Art(landscape))">$INFO[Container(99950).ListItem.Art(landscape)]</value>
		<value condition="$EXP[Standard_Channel] + !Player.HasMedia + !String.IsEmpty(Window(Home).Property(SkinHelper.ListItem.Art.Landscape))">$INFO[Window(Home).Property(SkinHelper.ListItem.Art.Landscape)]</value>
		<!-- 3. Fanart -->
		<value condition="$EXP[Standard_Channel] + !Player.HasMedia + !String.IsEmpty(Container(99950).ListItem.Property(base_fanart)">$INFO[Container(99950).ListItem.Property(base_fanart)]</value>
        <value condition="$EXP[Standard_Channel] + !Player.HasMedia + !String.IsEmpty(Container(99950).ListItem.Art(fanart))">$INFO[Container(99950).ListItem.Art(fanart)]</value>
		<value condition="$EXP[Standard_Channel] + !Player.HasMedia + !String.IsEmpty(Window(Home).Property(SkinHelper.ListItem.Art.Fanart))">$INFO[Window(Home).Property(SkinHelper.ListItem.Art.Fanart)]</value>
		<!-- 4. ClearLogo -->
        <value condition="$EXP[Standard_Channel] + !Player.HasMedia + !String.IsEmpty(Container(99950).ListItem.Property(base_clearlogo))">$INFO[Container(99950).ListItem.Property(base_clearlogo)]</value>
        <value condition="$EXP[Standard_Channel] + !Player.HasMedia + !String.IsEmpty(Container(99950).ListItem.Art(clearlogo))">$INFO[Container(99950).ListItem.Art(clearlogo)]</value>		
		<value condition="$EXP[Standard_Channel] + !Player.HasMedia + !String.IsEmpty(Window(Home).Property(SkinHelper.ListItem.Art.ClearLogo))">$INFO[Window(Home).Property(SkinHelper.ListItem.Art.ClearLogo)]</value>		
		<!-- 5. Thumb -->
		<value condition="$EXP[Standard_Channel] + !Player.HasMedia + !String.IsEmpty(Window(Home).Property(SkinHelper.ListItem.Art.Thumb))">$INFO[Window(Home).Property(SkinHelper.ListItem.Art.Thumb)]</value>
		<value condition="$EXP[Standard_Channel] + !Player.HasMedia + !String.IsEmpty(Container(99950).ListItem.Thumb)">$INFO[Container(99950).ListItem.Thumb]</value>			
		<!-- 6. Live Football Artwork -->			
		<value condition="[$EXP[Live_EPL_Anova] | $EXP[Live_EPL_FHNatho]] + !Player.HasMedia">special://userdata/customisations\PVR\EPG Artwork\EPL.jpg</value>
		<value condition="[$EXP[Live_EFLC_Anova] | $EXP[Live_EFLC_FHNatho]] + !Player.HasMedia">special://userdata/customisations\PVR\EPG Artwork\EFL.jpg</value>
		<value condition="[$EXP[Live_EFL1_Anova] | $EXP[Live_EFL1_FHNatho]] + !Player.HasMedia">special://userdata/customisations\PVR\EPG Artwork\EFL.jpg</value>
		<value condition="[$EXP[Live_EFL2_Anova] | $EXP[Live_EFL2_FHNatho]] + !Player.HasMedia">special://userdata/customisations\PVR\EPG Artwork\EFL.jpg</value>
		<value condition="[$EXP[Live_FAC_Anova] | $EXP[Live_FAC_FHNatho]] + !Player.HasMedia">special://userdata/customisations\PVR\EPG Artwork\FAC.jpg</value>			
		<value condition="$EXP[Live_NL_Anova] + !Player.HasMedia">special://userdata/customisations\PVR\EPG Artwork\NATIONAL LEAGUE.jpg</value>
		<value condition="$EXP[Live_EFLPJT_FHNatho] + !Player.HasMedia">special://userdata/customisations\PVR\EPG Artwork\PJT.jpg</value>
		<value condition="$EXP[Live_SPFLC_Anova] + !Player.HasMedia">special://userdata/customisations\PVR\EPG Artwork\SPFL.png</value>
		<value condition="$EXP[Live_SPFL_FHNatho] + !Player.HasMedia">special://userdata/customisations\PVR\EPG Artwork\SPFL.png</value>	
		<value condition="$EXP[Live_Football_Anova] + !Player.HasMedia">special://userdata/customisations\PVR\EPG Artwork\EFL.jpg</value>			
		<value condition="$EXP[Live_EFLPJT_FHNatho] + !Player.HasMedia">special://userdata/customisations\PVR\EPG Artwork\PJT.jpg</value>
				
		<!-- 9. Fallback -->
		<value condition="!Player.HasMedia">$INFO[ListItem.Icon]</value>
    </variable>
	
